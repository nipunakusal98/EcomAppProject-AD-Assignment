@model IEnumerable<EcomAppProject.Models.AntivirusSoftware>

@{
    ViewData["Title"] = "Index";
}

<h1>Antivirus Softwares</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AntivirusDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AntivirusPrice)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AntivirusDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AntivirusPrice)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AntivirusID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AntivirusID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AntivirusID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
