@*Define Model*@

@model IEnumerable<AntivirusSoftware>

@*Define ViewData*@

@{
    ViewData["Title"] = "List Of Anti-Virus Guards.";
}
<h1>Welcome to mmmmm</h1>

<div class="row">
    <div class="col-md-offset-md-2">
        <table class="table">
            <thead>
                <tr>
                    <th>@Html.DisplayNameFor(model=>model.AntivirusDescription)</th>
                    <th>@Html.DisplayNameFor(model=>model.AntivirusPrice)</th>
                    <th>Actions</th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                   
                        <td class="align-middle">
                            @Html.DisplayFor(modelItem => item.AntivirusDescription)
                        </td>
                        <td class="align-middle">
                            @Html.DisplayFor(modelItem => item.AntivirusPrice)
                        </td>
                        <td class="align-middle">
                            <a class="btn btn-outline-primary"><i class="bi bi-pencil-square"></i>Edit</a>
                            <a class="btn btn-outline-info"><i class="bi bi-eye"></i>Details</a>
                            <a class="btn btn-danger"><i class="bi bi-trash"></i>Delete</a>
                        </td>
                    </tr>
                }
            </tbody>

        </table>
    </div>
</div>
